{
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "types": [
      "node"
    ],
    "target": "es2020",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "strictNullChecks": true
  }
}
