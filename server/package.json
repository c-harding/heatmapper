{
  "name": "heatmapper-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run": "node index.js"
  },
  "type": "module",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-free-port": "^2.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}